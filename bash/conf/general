# set the default editor
export EDITOR=vim

# set the term to have 256 colors
export TERM=xterm-256color

#source the colors configuration
source "$HOME/.dots/bash/conf/colors"

#source the bashline configuration
source "$HOME/.dots/bash/conf/bashline"


export DYLD_LIBRARY_PATH="/usr/local/oracle/ohome/lib"
export TNS_ADMIN="/usr/local/oracle/ohome/network/admin/"
export ORACLE_HOME="/usr/local/oracle/ohome"
export PATH=/usr/local/bin:$PATH
